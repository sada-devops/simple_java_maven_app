node{
    
    stage("Git Checkout"){
        git 'https://github.com/srinivas1987devops/myweb.git'
        }
    stage("Maven Build"){
            sh '''mvn clean package
            mv target/*.war target/node-test.war'''
        }
    stage("deploy-dev"){
            sh 'cp **/*war /home/sada/apache-tomcat-9.0.63/webapps'
        }
}
